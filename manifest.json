{
    "manifest_version": 3,
    "name": "A Bot",
    "version": "1.4",
    "description": "Marks attendance on Canvas LMS",
    "icons": {
      "48": "/assets/icon.png"
    },
    "action":{
      "default_title":"A Bot",
      "default_popup":"index.html"
    },
    "permissions": [
      "tabs",
      "activeTab",
      "scripting",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["https://rollcall.instructure.com/sections/*"],
        "js": ["/scripts/jquery.js","/scripts/content_script.js"]
      },
      {
        "matches": ["https://moringa.instructure.com/courses/602/users"],
        "js": ["/scripts/browser-polyfill.min.js", "/scripts/at-risk.js"]
      }
    ]
  }
  